<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title - Analysis</title>
    <!--[if lt IE 10]>
      <script src="~/Content/assets/js/html5shiv.js"></script>
      <script src="~/Content/assets/js/respond.min.js"></script>
      <![endif]-->
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimal-ui">
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="description" content="Gradient Able Bootstrap admin template made using Bootstrap 4 and it has huge amount of ready made feature, UI components, pages which completely fulfills any dashboard needs." />
    <meta name="keywords" content="olap, pivot, pivotgrid, analysis, ssas, office, excel, flat ui, admin Admin, Responsive, Landing, Bootstrap, App, Template, Mobile, iOS, Android, apple, creative app">
    <meta name="author" content="nGenie DashLab" />
    <link rel="icon" href="~/Content/assets/images/favicon.ico" type="image/x-icon">
    <link href="~/Content/assets/css/roboto.css?family=Roboto:400,500" rel="stylesheet">

    @Styles.Render("~/bootstrap-css")
    @Styles.Render("~/waves-css")
    @Styles.Render("~/themify-css")
    @Styles.Render("~/font-awesome-css")

    <!-- Nuove -->
    @Styles.Render("~/dataTables-bootstrap4-css")
    @Styles.Render("~/buttons-dataTables-css")
    @Styles.Render("~/responsive-bootstrap4-css")

    @Styles.Render("~/style-css")
    @Styles.Render("~/mCustomScrollbar-css")

    <!-- Select2 CSS-->
    @Styles.Render("~/select2-css")
    @Styles.Render("~/bootstrap-multiselect-css")
    @Styles.Render("~/multiselect-css")

    @Scripts.Render("~/jquery-js")

    @RenderSection("CoreCssSection", required: false)
    @RenderSection("CustomCssSection", required: false)
    @RenderSection("CustomHeaderSection", required: false)

    <!-- Nuovo, serve per utilizzare la libreria signalR-->
    @Scripts.Render("~/jquery-signalR-js")
    <script src="~/signalr/hubs"></script>
</head>
<body>
    @Html.Partial("~/Views/Shared/_preloader.cshtml")

    <div id="pcoded" class="pcoded">
        <div class="pcoded-overlay-box"></div>
        <div class="pcoded-container navbar-wrapper">
            <!-- INI - TOP BAR AREA -->
            @Html.Partial("~/Views/Shared/NavBar/_headernavbar.cshtml")
            <!-- END - TOP BAR AREA -->
            <!-- INI - SIDEBAR AREA -->
            @Html.Partial("~/Views/Shared/SideBar/_pUserChat.cshtml")
            @Html.Partial("~/Views/Shared/SideBar/_pShowChat.cshtml")
            <!-- END - SIDEBAR AREA -->
            <!-- INI - CONTENT MAIN AREA -->
            <div class="pcoded-main-container">
                <div class="pcoded-wrapper">
                    <!-- INI - CONTENT MAIN LEFT MENU -->
                    @Html.Partial("~/Views/Shared/NavBar/_leftnavbar.cshtml")
                    <!-- END - CONTENT MAIN LEFT MENU -->
                    <!-- INI - CONTENT MAIN PAGE -->
                    <div class="pcoded-content">
                        @RenderBody()
                    </div>
                    <!-- END - CONTENT MAIN PAGE -->
                    <!-- INI - CONTENT MAIN RIGHT MENU -->
                    @Html.Partial("~/Views/Shared/NavBar/_rightnavbar.cshtml")
                    <!-- END - CONTENT MAIN RIGHT MENU -->
                </div>
            </div>
            <!-- END - CONTENT MAIN AREA -->
        </div>
    </div>

    @*<div class="navbar navbar-inverse navbar-fixed-top">
            <div class="container">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    @Html.ActionLink("Application name", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
                </div>
                <div class="navbar-collapse collapse">
                    <ul class="nav navbar-nav">
                        <li>@Html.ActionLink("Home", "Index", "Home")</li>
                        <li>@Html.ActionLink("About", "About", "Home")</li>
                        <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
                    </ul>
                    @Html.Partial("_LoginPartial")
                </div>
            </div>
        </div>
        <div class="container body-content">
            @RenderBody()
            <hr />
            <footer>
                <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
            </footer>
        </div>*@

