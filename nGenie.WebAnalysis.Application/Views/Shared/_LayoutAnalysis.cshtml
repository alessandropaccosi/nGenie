    @Scripts.Render("~/jquery-ui-js")

    @Scripts.Render("~/popper-js")
    @Scripts.Render("~/bootstrap-js")
    @Scripts.Render("~/waves-js")
    @Scripts.Render("~/jquery-slimscroll-js")

    @*Comprende anche css-scrollbars.js*@
    @Scripts.Render("~/modernizr-js")

    <!-- Nuove -->
    @Scripts.Render("~/jquery-dataTables-js")
    @Scripts.Render("~/datatables-buttons-js")
    @Scripts.Render("~/buttons-html5-js")
    @Scripts.Render("~/dataTables-bootstrap4-js")
    @Scripts.Render("~/dataTables-responsive-js")
    @Scripts.Render("~/responsive-bootstrap4-js")

    @Scripts.Render("~/pcoded-js")
    @Scripts.Render("~/vertical-js")
    @Scripts.Render("~/mCustomScrollbar-js")
    @Scripts.Render("~/script-js")
    @Scripts.Render("~/bootbox-js")

    @*  Libreria Toastr @@@ verificare se occorre anche toastr.js.map *@
    @Scripts.Render("~/toastr-js")
    @Styles.Render("~/toastr-css")




    <!-- Select2 JS -->
    @*@Scripts.Render("~/jquery-ui-js")*@
    @Scripts.Render("~/select2-js")
    @Scripts.Render("~/bootstrap-multiselect-js")
    @Scripts.Render("~/multiselect-js")  @*jquery multiselect*@
    @Scripts.Render("~/jquery-quicksearch-js")
    @Scripts.Render("~/select2-custom-js")
    <!-- Fine Select2 JS -->


    @RenderSection("scripts", required: false)
    @RenderSection("CoreJs", required: false)
    @RenderSection("CustomJs", required: false)
</body>
</html>







